{
  "name": "@canlooks/ajax",
  "version": "3.3.0",
  "description": "A private tool",
  "author": "C.CanLiang <canlooks@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ajax",
    "request",
    "xhr",
    "XMLHttpRequest",
    "modularization",
    "react",
    "vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./index.d.ts"
    },
    "./react": {
      "import": "./react/index.js",
      "require": "./react/index.cjs",
      "types": "./react/index.d.ts"
    },
    "./vue": {
      "import": "./vue/index.js",
      "require": "./vue/index.cjs",
      "types": "./vue/index.d.ts"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "shx rm -rf dist && tsc -p tsconfig.esm.json & tsc -p tsconfig.cjs.json",
    "publish": "npm publish",
    "test:node": "ts-node test/test.ts",
    "test:browser": "vite -c test/vite.config.mts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/canlooks/c_ajax"
  },
  "homepage": "https://gitee.com/canlooks/c_ajax",
  "bugs": {
    "url": "https://gitee.com/canlooks/c_ajax/issues",
    "email": "canlooks@gmail.com"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^5.0.8",
    "vue": "^3.3.4"
  }
}
