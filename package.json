{
  "name": "@canlooks/ajax",
  "version": "3.1.1",
  "description": "A private tool",
  "author": "C.CanLiang <canlooks@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ajax",
    "request",
    "modularization",
    "react"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./index.d.ts"
    },
    "./react": {
      "import": "./react/index.js",
      "require": "./react/index.cjs",
      "types": "./react/index.d.ts"
    },
    "./dist/*/http.js": {
      "browser": {
        "import": "./dist/esm/xhr.js",
        "require": "./dist/cjs/xhr.js"
      },
      "default": {
        "import": "./dist/esm/http.js",
        "require": "./dist/cjs/http.js"
      }
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "browser": {
    "./dist/cjs/http.js": "./dist/cjs/xhr.js",
    "./dist/esm/http.js": "./dist/esm/xhr.js"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "test": "mocha -r ts-node/register test/test.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/canlooks/c_ajax"
  },
  "homepage": "https://gitee.com/canlooks/c_ajax",
  "bugs": {
    "url": "https://gitee.com/canlooks/c_ajax/issues",
    "email": "canlooks@gmail.com"
  },
  "dependencies": {
    "follow-redirects": "^1.15.2"
  },
  "devDependencies": {
    "@types/follow-redirects": "^1.14.1",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
