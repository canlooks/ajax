{
  "name": "@canlooks/ajax",
  "version": "3.0.3",
  "description": "A private tool",
  "author": "C.CanLiang <canlooks@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ajax",
    "request",
    "modularization",
    "react"
  ],
  "exports": {
    "./react": {
      "import": "./react/index.js",
      "require": "./react/index.cjs"
    }
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "index.d.ts",
  "browser": {
    "./dist/http.js": "./dist/xhr.js"
  },
  "scripts": {
    "clean": "shx rm -rf dist react/index.d.ts react/index.js",
    "build": "npm run clean && tsc -p tsconfig.es.json && tsc -p tsconfig.lib.json",
    "test": "mocha -r ts-node/register test/test.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/canlooks/c_ajax"
  },
  "homepage": "https://gitee.com/canlooks/c_ajax",
  "bugs": {
    "url": "https://gitee.com/canlooks/c_ajax/issues",
    "email": "canlooks@gmail.com"
  },
  "dependencies": {
    "follow-redirects": "^1.15.2"
  },
  "devDependencies": {
    "@types/follow-redirects": "^1.14.1",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
